@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "Admin Dashboard";
}
<link rel="stylesheet" href="~/Admin/dist/css/button_menu.css">
<link rel="stylesheet" href="~/Admin/dist/css/reponsive_dashboard.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" integrity="sha512-..." crossorigin="anonymous" />
<link rel="stylesheet" href="path/to/font-awesome/css/all.min.css" />


<h2 class="hello_dashboard">Xin Chào!<span style="color:red; font-weight: bold;"> @User.Identity.Name</span></h2>

<!-- Small boxes (Stat box) -->
<div class="row">
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h3>@ViewBag.orderCount</h3>

                <p>Đơn Hàng Mới</p>
            </div>

        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-success">
            <div class="inner">
                <h3>@ViewBag.productCount</h3>

                <p>Sản Phẩm</p>
            </div>

        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-warning">
            <div class="inner">
                <h3>@ViewBag.userCount</h3>

                <p>Số Lượng Người Dùng</p>
            </div>

        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-danger">
            <div class="inner">
                <h3>@ViewBag.onlineUser</h3>

                <p>Số lượng khách truy cập</p>
            </div>

        </div>
    </div>
    <!-- ./col -->
</div>
<!--<a asp-controller="EmailData" asp-action="SendEmail" class="btn btn-outline-info mt-2">Go to Send Email</a>-->
<a asp-area="Admin" asp-controller="EmailData" asp-action="SendEmail" class="btn mt-2" style="border: none;">
    <!--Go to Send Email-->
    <img class="logo_gmail" style="width: 160px; height: 120px; cursor: pointer;" src="https://upload.wikimedia.org/wikipedia/commons/thumb/7/7e/Gmail_icon_%282020%29.svg/512px-Gmail_icon_%282020%29.svg.png" alt="send_email" />
</a>

